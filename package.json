{
    "name": "@abusix/hailstorm",
    "version": "1.11.0-next.1",
    "description": "our own design/component library",
    "homepage": "https://github.com/abusix/hailstorm",
    "author": "abusix",
    "license": "ISC",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abusix/hailstorm.git"
    },
    "bugs": {
        "url": "https://github.com/abusix/hailstorm/issues"
    },
    "scripts": {
        "storybook": "storybook dev -p 6006",
        "clean": "rimraf ./dist",
        "type-check": "tsc --project tsconfig.json",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:write": "eslint src --ext .ts,.tsx --fix",
        "format:check": "prettier --check src",
        "format:write": "prettier --write src",
        "build": "npm run clean && npm run build:rollup",
        "build:rollup": "NODE_ENV=production rollup -c",
        "build:icons": "svgr -- assets/icons",
        "build:storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook build",
        "build:tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/styles/index.css --minify"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.15",
        "@headlessui/tailwindcss": "^0.1.3",
        "@tailwindcss/forms": "^0.5.3",
        "@tanstack/react-table": "^8.9.3",
        "@tanstack/react-virtual": "^3.0.0-beta.54",
        "@tanstack/table-core": "^8.9.3",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "tslib": "^2.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.8",
        "@babel/preset-env": "^7.22.7",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@rollup/plugin-commonjs": "^25.0.2",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.2",
        "@semantic-release/git": "^10.0.1",
        "@storybook/addon-docs": "^7.1.1",
        "@storybook/addon-essentials": "^7.1.1",
        "@storybook/addon-interactions": "^7.1.1",
        "@storybook/addon-links": "^7.1.1",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/blocks": "^7.1.1",
        "@storybook/react": "^7.1.1",
        "@storybook/react-vite": "^7.1.1",
        "@storybook/testing-library": "^0.2.0",
        "@svgr/cli": "8.0.1",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.2",
        "@types/react": "^18.2.14",
        "@typescript-eslint/eslint-plugin": "5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "autoprefixer": "^10.4.14",
        "babel-jest": "^29.6.1",
        "babel-loader": "^9.1.2",
        "eslint": "8.44.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "17.0.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-prettier": "^5.0.0-alpha.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-storybook": "0.6.13",
        "eslint-plugin-tsdoc": "0.2.17",
        "jest": "^29.6.1",
        "jest-environment-jsdom": "^29.6.1",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "0.3.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.1",
        "rollup": "^3.26.2",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-tailwindcss": "^1.0.0",
        "semantic-release": "^21.0.7",
        "storybook": "^7.1.1",
        "tailwind-merge": "^1.13.2",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.1.6",
        "vite": "^4.4.1"
    },
    "peerDependencies": {
        "react": ">=17.0.1",
        "react-dom": ">=17.0.1"
    },
    "overrides": {
        "optionator": {
            "word-wrap@1.2.3": "1.2.5"
        },
        "jsdom@20": {
            "tough-cookie@4.1.2": "4.1.3"
        }
    }
}
